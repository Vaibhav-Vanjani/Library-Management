generator client {
  provider = "prisma-client"
  output   = "../generated/prisma-entry-exit"
}

datasource db {
  provider = "mysql"
}

model entryExit{
  id Int @id @default(autoincrement())
  userId String @unique
  entryTime String
  exitTime String
  currentDate String
  isActive Boolean
}
